<div class="userslist_box flex-row-start-start">
        <% if @all_users.present? %>
            <% @all_users.each do |item| %>
                <div class="userdata_box flex-col-start-start">
                    <div><%= item.email %></div>
                    <div><%= item.username %></div>
                    <%= button_to "Delete", user_registration_path, method: :delete, class: "btn btn-danger", style: "margin:0px 4px;" %>
                </div>
            <% end %>
        <% else %>
            <div class="userdata_box flex-row-center-center">No users yet</div>
        <% end %>
</div>